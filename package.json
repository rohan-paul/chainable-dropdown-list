{
  "name": "ReactDropDown",
  "version": "1.0.0",
  "description": "A chained dropdown UI with React",
  "main": "server.js",
  "scripts": {
    "client-install": "npm start --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "author": "Rohan Paul",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "concurrently": "^3.6.0",
    "cors": "^2.8.4",
    "expect": "^23.2.0",
    "express": "^4.16.3",
    "html-webpack-plugin": "^3.2.0",
    "mongoose": "^5.1.7",
    "webpack": "^4.12.2"
  },
  "devDependencies": {
    "nodemon": "^1.17.5",
    "webpack-cli": "^3.0.8",
    "karma": "^2.0.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "mocha": "^5.2.0"
  }
}
